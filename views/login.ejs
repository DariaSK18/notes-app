<%- include('blocks/head') -%>
<body>
    <div class="wrapper">
        <%- include('blocks/header') -%>
        <div class="main">
            <h2>Log In</h2>
            <form id="loginForm" >
                <label>
                    Username: 
                    <input type="text" name="userName" placeholder="Username" required>
                </label>
                <label>
                    Password: 
                    <input type="password" name="password" placeholder="Password" required>
                </label>
                <button type="submit">LogIn</button>
            </form>
        </div>
    </div>
</body>